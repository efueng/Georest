{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=secretSanta.sqlite"
  },
  "Serilog": {
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "SQLite",
        "Args": {
          "sqliteDbPath": "..\\..\\..\\secretSanta.sqlite",
          "storeTimestampUtc": true
        }
      }
    ]
  }
}