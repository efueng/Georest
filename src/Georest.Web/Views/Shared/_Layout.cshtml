<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>@ViewBag.Title</title>
    <!-- Stylesheets -->
    
    <link href="~/css/main.9d7a3924ddb8e24a327e.css" rel="stylesheet" />
    
    <!-- End Stylesheets -->
</head>
<body>
    <nav class="navbar" role="navigation" aria-label="main navigation">
        <div class="navbar-brand">
            <a class="navbar-item" asp-controller="Home" asp-action="Index">
                Geolabs
            </a>
        </div>

        <div id="navbarBasicExample" class="navbar-menu">
            <div class="navbar-start">
                <a class="navbar-item" asp-controller="Students" asp-action="Index">
                    Students
                </a>
                <a class="navbar-item" asp-controller="Instructors" asp-action="Index">
                    Instructors
                </a>
                <a class="navbar-item" asp-controller="InstructorLabs" asp-action="Index">
                    Labs
                </a>
                <a class="navbar-item" asp-controller="Exercises" asp-action="Index">
                    Exercises
                </a>
            </div>
        </div>
    </nav>
    <section class="section">
        <div class="container">
            @RenderBody()
        </div>
    </section>

    <!-- Scripts -->
    
    <script src="~/js/main.9d7a3924ddb8e24a327e.js"></script>
    
    <!-- End Scripts -->
    @RenderSection("scripts", required: false)
</body>
</html>
